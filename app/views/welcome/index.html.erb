<h1>Welcome to Algorhythms</h1>

<table>
  <% @qr.modules.each_index do |x| %>
    <tr>
      <% @qr.modules.each_index do |y| %>
        <% if @qr.dark?(x,y) %>
          <td class="black"/>
        <% else %>
          <td class="white"/>
        <% end %>
      <% end %>
    </tr>
  <% end %>
</table>

<% if @it_worked %>
  <p>IT WORKED!</p>
<% end %>
